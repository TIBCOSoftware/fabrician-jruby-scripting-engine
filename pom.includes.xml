<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.datasynapse.fabricserver.scriptengines</groupId>
	<artifactId>includes</artifactId>
	<name></name>
	<packaging>pom</packaging>
	<version>1.0.0.1</version>
	<description></description>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
	<build> 
		<plugins>  
            <plugin>
                <groupId>com.datasynapse.maven-plugins</groupId>
                <artifactId>datasynapse-release-plugin</artifactId>
                <version>0.11</version>
                <configuration>
                    <releaseArguments>-f ${pomName} deploy</releaseArguments>
                    <connectionUrl>http://svn/svn/srcrepos/modules/fabric-scripting/trunk</connectionUrl>
                    <tagBase>http://svn/svn/srcrepos/modules/fabric-scripting/tags</tagBase>
                </configuration>
            </plugin>
		</plugins>
	</build>
    <distributionManagement>
        <snapshotRepository>
            <id>datasynapse-snapshot</id>
            <name>DataSynapse Repository</name>
            <url>http://gibson:8081/nexus/content/repositories/datasynapse-snapshot</url>
        </snapshotRepository>
        <repository>
            <id>datasynapse-release</id>
            <name>DataSynapse Repository</name>
            <url>http://gibson:8081/nexus/content/repositories/datasynapse-release</url>
        </repository>
    </distributionManagement>
</project>
