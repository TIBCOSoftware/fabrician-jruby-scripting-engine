<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>gridlib</id>
	<baseDirectory></baseDirectory>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>${gridlib-dir}</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>**</include>
			</includes>
			<excludes>
                <exclude>grid-library.xml</exclude>
			</excludes>
            <filtered>false</filtered>           
		</fileSet>
        <fileSet>
            <directory>${gridlib-dir}</directory>
            <outputDirectory></outputDirectory>
            <includes>
                <include>grid-library.xml</include>
            </includes>
            <filtered>true</filtered>           
        </fileSet>
	</fileSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
            <unpack>false</unpack>
            <includes>
               <include>org.jruby:jruby-complete</include>
            </includes>
        </dependencySet>
  </dependencySets>
</assembly>
